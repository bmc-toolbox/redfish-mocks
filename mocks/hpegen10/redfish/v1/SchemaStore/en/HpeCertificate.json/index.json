{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "#HpeCertificate.v1_0_0.HpeCertificate",
    "type": "object",
    "readonly": false,
    "additionalProperties": false,
    "description": "This is the schema definition for X509 certificates.",
    "properties": {
        "Subject": {
            "type": "string",
            "readonly": true,
            "additionalProperties": false,
            "description": "The entity to which the certificate was issued.",
            "etag": true
        },
        "Issuer": {
            "type": "string",
            "readonly": true,
            "additionalProperties": false,
            "description": "The Certificate Authority that issued the certificate.",
            "etag": true
        },
        "ValidNotBefore": {
            "type": [
                "string",
                "null"
            ],
            "format": "date-time",
            "readonly": true,
            "additionalProperties": false,
            "description": "The date on which the certificate validity period begins.",
            "etag": true
        },
        "ValidNotAfter": {
            "type": [
                "string",
                "null"
            ],
            "format": "date-time",
            "readonly": true,
            "additionalProperties": false,
            "description": "The date on which the certificate validity period ends.",
            "etag": true
        },
        "SerialNumber": {
            "type": "string",
            "readonly": true,
            "additionalProperties": false,
            "description": "The serial number that the Certificate Authority assigned to the certificate.",
            "etag": true
        },
        "@odata.id": {
            "$ref": "Resource.json#/definitions/odataid"
        },
        "@odata.context": {
            "$ref": "Resource.json#/definitions/odatacontext"
        },
        "@odata.type": {
            "$ref": "Resource.json#/definitions/odatatype"
        },
        "@odata.etag": {
            "$ref": "Resource.json#/definitions/odataetag"
        },
        "Id": {
            "$ref": "Resource.json#/definitions/Id"
        }
    },
