            "description": "Describes the various options to perform Jitter Control of the processor",
            "properties": {
                "Mode": {
                    "description": "Provides various jitter control modes.",
                    "etag": true,
                    "readonly": false,
                    "type": "string",
                    "enum": [
                        "Auto",
                        "Disabled",
                        "Manual"
                    ],
                    "enumDescription": {
                        "Auto": "When configured in Auto mode, the platform will monitor frequency variance of the processor and automatically make adjustments to minimize variance over time.",
                        "Disabled": "Jitter control is disabled.",
                        "Manual": "When configured in Manual mode, the customer can choose to attempt to operate the processor at a fixed frequency."
                    }
                },
                "FrequencyLimitMHz": {
                    "description": "Current frequency limit of the processor in MHz.",
